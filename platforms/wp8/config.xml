<?xml version='1.0' encoding='utf-8'?>
<widget id="com.covadis.covadis" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="WebSql">
        <param name="wp-package" value="WebSql" />
    </feature>
    <feature name="SplashScreen">
        <param name="wp-package" value="SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>coVadis</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <feature name="Device">
        <param name="wp-package" value="Device" />
    </feature>
    <feature name="LocalNotification">
        <param name="wp-package" value="LocalNotification" />
    </feature>
</widget>
